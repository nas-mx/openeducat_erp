<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_frontend_js_script" inherit_id="website.assets_frontend" name="lms Frontend Assets">
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/openeducat_lms_website/static/src/js/course_section_add.js"/>
            <script type="text/javascript" src="/openeducat_lms_website/static/src/js/material_upload.js"/>
        </xpath>
    </template>

    <template id="assets_editor_inherit_lms" inherit_id="website.assets_editor"
              name="openeducat_lms Assets Editor">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/openeducat_lms_website/static/src/js/lms_course.editor.js"/>
        </xpath>
    </template>
</odoo>
