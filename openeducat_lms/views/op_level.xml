<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="view_op_level_tree" model="ir.ui.view">
            <field name="name">op.level.tree</field>
            <field name="model">op.level</field>
            <field name="type">tree</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <tree string="Level">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="view_op_level_form" model="ir.ui.view">
            <field name="name">op.level.form</field>
            <field name="model">op.level</field>
            <field name="type">form</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="Level">
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="act_open_op_level_view">
            <field name="name">Level</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">op.level</field>
            <field name="binding_view_types">form</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>
